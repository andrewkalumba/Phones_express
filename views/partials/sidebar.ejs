<div class="sidebar">
    <h2>Available Only in Store</h2>
    <ul>
        <% phones.forEach(phone => { %>
            <li>
                <a class="phone" href="/?phone=<%= encodeURIComponent(phone.name) %>">
                    <%= phone.name %>
                </a>
            </li>
        <% }); %>
    </ul>
</div>


